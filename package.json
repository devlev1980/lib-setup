{
  "name": "lib-setup",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ",
    "build:lib": "ng build carousel",
    "buid:lib:watch": "ng build carousel --watch",
    "build:showcase": "ng build carousel-showcase --prod",
    "build": "npm run build:lib && npm run build:showcase",
    "postbuild:lib": "npm run generate:toc && cp README.md dist/carousel/README.md && cp LICENSE.md dist/carousel/LICENSE.md",
    "watch": "ng build --watch --configuration development",
    "format-lib:check": "prettier --check \"projects/carousel/**/*.{ts,js,json,html,scss}\"",
    "format-showcase:check": "prettier --check \"projects/carousel-showcase/**/*.{ts,js,json,html,scss}\"",
    "format:check": "npm run format-lib:check && npm run format-showcase:check",
    "format-lib:write": "prettier --write \"projects/carousel/**/*.{ts,js,json,html,scss}\"",
    "format-showcase:write": "prettier --write \"projects/carousel-showcase/**/*.{ts,js,json,html,scss}\"",
    "format:write": "npm run format-lib:write && npm run format-showcase:write",
    "generate:toc": "npx doctoc README.md",
    "test": "ng test",
    "serve:ssr:carousel-showcase": "node dist/carousel-showcase/server/server.mjs",
    "lint": "npm run lint:lib && npm run lint:showcase",
    "lint:lib": "eslint --ext .js,.ts ./projects/carousel-showcase/src",
    "lint:showcase": "eslint --ext .js,.ts ./projects/carousel-showcase/src"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/ssr": "^17.0.3",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular-eslint/eslint-plugin-template": "17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular-eslint/template-parser": "17.1.1",
    "@angular/cli": "^17.0.3",
    "@angular/compiler-cli": "^17.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "all-contributors-cli": "^6.26.1",
    "eslint": "^8.54.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.0.0",
    "prettier": "^3.1.0",
    "typescript": "~5.2.2"
  }
}
